{
  "name": "ai-assistant",
  "version": "0.1.0",
  "main": "dist-electron/main/index.js",
  "description": "Electron + Vue + Vite + Tailwind + i18n + EventBus boilerplate.",
  "author": "richard",
  "license": "MIT",
  "private": true,
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue",
    "Tailwind"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "preview": "vite preview",
    "generate-icons": "electron-icon-builder --input=./public/chatbot.png --output=./public --flatten"
  },
  "devDependencies": {
    "@electron/asar": "^3.2.4",
    "@types/crypto-js": "^4.1.1",
    "@types/prismjs": "^1.26.0",
    "@types/uuid": "^9.0.2",
    "@types/vue-i18n": "^7.0.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "autoprefixer": "^10.4.14",
    "electron": "^25.0.1",
    "electron-builder": "^24.4.0",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.1.4",
    "vite-plugin-electron": "^0.12.0",
    "vite-plugin-electron-renderer": "^0.14.1",
    "vite-plugin-prismjs": "^0.0.8",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue": "^3.2.47",
    "vue-tsc": "^1.2.0"
  },
  "dependencies": {
    "crypto-js": "^4.1.1",
    "halcyonui": "^0.1.1",
    "mitt": "^3.0.1",
    "moment": "^2.29.4",
    "prismjs": "^1.29.0",
    "uuid": "^9.0.0",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.4"
  }
}
